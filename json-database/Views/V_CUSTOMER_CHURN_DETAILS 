WITH CTE_CUSTOMER_CHURN_DETAILS 
AS
(
SELECT	  KUNDE	
	, LETZTE_TIMEBASE
	, I_ALIVE	
	, CHURN_WAHRSCHEINLICHKEIT
	, "THRESHOLD_STANDARD_0.5" AS THRESHOLD_STANDARD
	, "PREDICTED_STANDARD_0.5" AS PREDICTED_STANDARD	
	, THRESHOLD_OPTIMAL	
	, PREDICTED_OPTIMAL
	, THRESHOLD_ELBOW	
	, PREDICTED_ELBOW
	, THRESHOLD_F1_OPTIMAL	
	, PREDICTED_F1_OPTIMAL	
	, PREDICTED_PRECISION_FIRST	
	, THRESHOLD_RECALL_FIRST
	, PREDICTED_RECALL_FIRST	
	, E.TRAINING_FROM
	, E.TRAINING_TO
	, E.BACKTEST_FROM
	, E.BACKTEST_TO
	, E.EXPERIMENT_ID	
	FROM CUSTOMER_CHURN_DETAILS D
	JOIN EXPERIMENTS E ON E.EXPERIMENT_ID = D.EXPERIMENT_ID
)
SELECT * 
FROM CTE_CUSTOMER_CHURN_DETAILS 